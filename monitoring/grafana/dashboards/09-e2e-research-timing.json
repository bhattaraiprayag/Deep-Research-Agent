{
  "id": null,
  "uid": "e2e-research-timing",
  "title": "üïê End-to-End Research Timing",
  "description": "Decompose the full research lifecycle into measurable phases",
  "tags": [
    "agent",
    "timing",
    "latency",
    "p3"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Avg Research Duration",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(research_duration_seconds) or vector(0)",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 60,
                "color": "yellow"
              },
              {
                "value": 180,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Research Duration P95",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(research_duration_seconds_bucket[1h])) by (le))",
          "legendFormat": "P95",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 120,
                "color": "yellow"
              },
              {
                "value": 300,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "SLA Compliance (<2 min)",
      "description": "Tasks completing under 2 minutes",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(research_duration_seconds_bucket{le=\"120\"}[1h])) / sum(rate(research_duration_seconds_count[1h])) or vector(0)",
          "legendFormat": "SLA Compliance",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 0.7,
                "color": "yellow"
              },
              {
                "value": 0.9,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Tasks Completed (24h)",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(increase(research_tasks_total{status=\"completed\"}[24h])) or vector(0)",
          "legendFormat": "Completed",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Duration Distribution",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(research_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, sum(rate(research_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P75",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(research_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P90",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(research_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "D"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(research_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99",
          "refId": "E"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 5
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Time by Phase",
      "description": "Breakdown of time spent in each phase",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(phase_duration_seconds{phase=\"planning\"})",
          "legendFormat": "Planning (Strategist)",
          "refId": "A"
        },
        {
          "expr": "avg(phase_duration_seconds{phase=\"searching\"})",
          "legendFormat": "Searching (Hunter)",
          "refId": "B"
        },
        {
          "expr": "avg(phase_duration_seconds{phase=\"extracting\"})",
          "legendFormat": "Extracting (Curator)",
          "refId": "C"
        },
        {
          "expr": "avg(phase_duration_seconds{phase=\"writing\"})",
          "legendFormat": "Writing (Analyst)",
          "refId": "D"
        },
        {
          "expr": "avg(phase_duration_seconds{phase=\"reviewing\"})",
          "legendFormat": "Reviewing (Critic)",
          "refId": "E"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal",
              "group": "A"
            }
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Phase Time % Breakdown",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(increase(phase_duration_seconds{phase=\"planning\"}[1h]))",
          "legendFormat": "Planning",
          "refId": "A"
        },
        {
          "expr": "sum(increase(phase_duration_seconds{phase=\"searching\"}[1h]))",
          "legendFormat": "Searching",
          "refId": "B"
        },
        {
          "expr": "sum(increase(phase_duration_seconds{phase=\"extracting\"}[1h]))",
          "legendFormat": "Extracting",
          "refId": "C"
        },
        {
          "expr": "sum(increase(phase_duration_seconds{phase=\"writing\"}[1h]))",
          "legendFormat": "Writing",
          "refId": "D"
        },
        {
          "expr": "sum(increase(phase_duration_seconds{phase=\"reviewing\"}[1h]))",
          "legendFormat": "Reviewing",
          "refId": "E"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "donut",
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 8,
      "title": "Duration Trend (7 Days)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg_over_time(avg(research_duration_seconds)[1h])",
          "legendFormat": "Avg Duration",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "thresholdsStyle": {
              "mode": "area"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 60,
                "color": "yellow"
              },
              {
                "value": 120,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Task Completion Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(research_tasks_total{status=\"completed\"}[5m]))",
          "legendFormat": "Completed/sec",
          "refId": "A"
        },
        {
          "expr": "sum(rate(research_tasks_total{status=\"failed\"}[5m]))",
          "legendFormat": "Failed/sec",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Completed/sec"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failed/sec"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      }
    ]
  },
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
