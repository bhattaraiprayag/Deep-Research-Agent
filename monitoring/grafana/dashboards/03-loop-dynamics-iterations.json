{
  "id": null,
  "uid": "loop-dynamics-iterations",
  "title": "ðŸ”„ Loop Dynamics & Iteration Analytics",
  "description": "Visualize the agent's iterative behaviorâ€”research loops and critique loops",
  "tags": [
    "agent",
    "loops",
    "iterations",
    "p1"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Avg Iterations per Task",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(research_iterations_total) or vector(0)",
          "legendFormat": "Avg Iterations",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 3,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Max Iterations Hits (24h)",
      "description": "Tasks that hit MAX_ITERATIONS cap (default: 5)",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(increase(research_max_iterations_reached_total[24h])) or vector(0)",
          "legendFormat": "Max Hits",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "First-Pass Approval Rate",
      "description": "Reports approved by Critic without revision",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(critique_decisions_total{outcome=\"approved\", attempt=\"1\"}[1h])) / sum(rate(critique_decisions_total{attempt=\"1\"}[1h])) or vector(0)",
          "legendFormat": "First-Pass Rate",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 0.7,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Avg Critique Loops",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(critique_loops_total) or vector(0)",
          "legendFormat": "Avg Loops",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 2,
                "color": "yellow"
              },
              {
                "value": 3,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Research Iterations Distribution",
      "type": "histogram",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by(le) (increase(research_iteration_count_bucket[1h]))",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "options": {
        "bucketSize": 1,
        "bucketOffset": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 80,
            "gradientMode": "scheme",
            "lineWidth": 1
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Critique Loop Distribution",
      "type": "histogram",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by(le) (increase(critique_loop_count_bucket[1h]))",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "options": {
        "bucketSize": 1,
        "bucketOffset": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "fillOpacity": 80,
            "gradientMode": "scheme",
            "lineWidth": 1
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Iteration Count Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(research_iterations_total)",
          "legendFormat": "Avg Iterations",
          "refId": "A"
        },
        {
          "expr": "max(research_iterations_total)",
          "legendFormat": "Max Iterations",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "thresholdsStyle": {
              "mode": "dashed"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Critique Decisions Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(critique_decisions_total{outcome=\"approved\"}[5m]))",
          "legendFormat": "Approved",
          "refId": "A"
        },
        {
          "expr": "sum(rate(critique_decisions_total{outcome=\"rejected\"}[5m]))",
          "legendFormat": "Rejected",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal",
              "group": "A"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Approved"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Rejected"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 9,
      "title": "Loop Efficiency Score",
      "description": "Tasks completed with fewer than half of max iterations",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(research_tasks_total{iterations_bucket=\"low\"}[1h])) / sum(rate(research_tasks_total[1h])) or vector(0.5)",
          "legendFormat": "Efficiency",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 0.4,
                "color": "yellow"
              },
              {
                "value": 0.7,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Research Progress Rate",
      "description": "How quickly does fact gathering progress per iteration?",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 16,
        "x": 8,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(facts_gathered_total[5m])) / sum(rate(research_iterations_total[5m])) or vector(0)",
          "legendFormat": "Facts per Iteration",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}
