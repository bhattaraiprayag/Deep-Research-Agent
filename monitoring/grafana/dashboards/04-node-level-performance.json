{
  "id": null,
  "uid": "node-level-performance",
  "title": "ðŸ“Š Node-Level Performance Breakdown",
  "description": "Granular latency and success metrics for each of the 5 LangGraph nodes",
  "tags": [
    "agent",
    "nodes",
    "performance",
    "p1"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Strategist Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(node_executions_total{node=\"strategist\"}[5m])) or vector(0)",
          "legendFormat": "Calls/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 3,
          "color": {
            "mode": "fixed",
            "fixedColor": "blue"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Hunter Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 4,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(node_executions_total{node=\"hunter\"}[5m])) or vector(0)",
          "legendFormat": "Calls/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 3,
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Curator Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(node_executions_total{node=\"curator\"}[5m])) or vector(0)",
          "legendFormat": "Calls/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 3,
          "color": {
            "mode": "fixed",
            "fixedColor": "yellow"
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Analyst Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(node_executions_total{node=\"analyst\"}[5m])) or vector(0)",
          "legendFormat": "Calls/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 3,
          "color": {
            "mode": "fixed",
            "fixedColor": "orange"
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Critic Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(node_executions_total{node=\"critic\"}[5m])) or vector(0)",
          "legendFormat": "Calls/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 3,
          "color": {
            "mode": "fixed",
            "fixedColor": "purple"
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Slowest Node",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 20,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "topk(1, avg by(node) (rate(node_execution_duration_seconds_sum[5m]) / rate(node_execution_duration_seconds_count[5m])))",
          "legendFormat": "{{node}}",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "none",
        "orientation": "horizontal",
        "textMode": "name",
        "colorMode": "background",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Node Execution Time (P50/P95/P99)",
      "type": "timeseries",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by(node, le) (rate(node_execution_duration_seconds_bucket[5m])))",
          "legendFormat": "{{node}} P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by(node, le) (rate(node_execution_duration_seconds_bucket[5m])))",
          "legendFormat": "{{node}} P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by(node, le) (rate(node_execution_duration_seconds_bucket[5m])))",
          "legendFormat": "{{node}} P99",
          "refId": "C"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "max"
          ]
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 0,
            "pointSize": 5
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Node Call Count (Rate)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by(node) (rate(node_executions_total[5m]))",
          "legendFormat": "{{node}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal",
              "group": "A"
            }
          }
        }
      }
    },
    {
      "id": 9,
      "title": "Node Error Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by(node) (rate(node_errors_total[5m])) / sum by(node) (rate(node_executions_total[5m])) * 100 or vector(0)",
          "legendFormat": "{{node}} Error %",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Node Execution Distribution (24h)",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by(node) (increase(node_executions_total[24h]))",
          "legendFormat": "{{node}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 11,
      "title": "Average Node Duration",
      "type": "bargauge",
      "gridPos": {
        "h": 8,
        "w": 16,
        "x": 8,
        "y": 22
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg by(node) (rate(node_execution_duration_seconds_sum[5m]) / rate(node_execution_duration_seconds_count[5m]))",
          "legendFormat": "{{node}}",
          "refId": "A"
        }
      ],
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true,
        "minVizWidth": 10,
        "minVizHeight": 10
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 15,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 12,
      "title": "Hunter Search Concurrency",
      "description": "Number of parallel searches in flight",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 30
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "hunter_searches_in_flight or vector(0)",
          "legendFormat": "Concurrent Searches",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 30
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Curator Facts Extracted",
      "description": "Facts extracted per batch by Curator node",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 30
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "rate(curator_facts_extracted_total[5m])",
          "legendFormat": "Facts/sec",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 30
          }
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [],
        "refresh": 1,
        "regex": ""
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
